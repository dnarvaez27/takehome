[MAIN]
ignored-modules=shared,
jobs=1
py-version=3.9


[DESIGN]
exclude-too-few-public-methods=
ignored-parents=
max-args=15
max-attributes=7
max-bool-expr=5
max-branches=12
max-locals=20
max-parents=7
max-returns=6
max-statements=50
min-public-methods=1


[FORMAT]
expected-line-ending-format=LF
max-line-length=120
max-module-lines=1000


[STRING]
check-quote-consistency=yes


[BASIC]
good-names=i,
           j,
           k,
           n,
           r,
           e,
           v,
           id,
           ex,
           Run,
           ip,
           db,
           _


[EXCEPTIONS]
overgeneral-exceptions=


[VARIABLES]
allowed-redefined-builtins=id,
                           type,


[TYPECHECK]
generated-members=boto3.resource
ignored-classes=boto3.resource

[MESSAGES CONTROL]
disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        missing-docstring,
        fixme,
        duplicate-code,
        too-many-instance-attributes,
        too-few-public-methods,
        invalid-unary-operand-type,
        too-many-return-statements,
        too-many-public-methods,


[MASTER]
extension-pkg-whitelist=pydantic
